{
    "root": true,
    "extends": ["eslint:recommended", "plugin:import/warnings"],
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "rules": {
      "no-debugger": "warn",
      "no-console": "warn",
      "import/extensions": [1, { "js": "never" }]
    },
    "parserOptions": {
        "sourceType": "module"
    }
  }